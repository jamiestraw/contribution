{
  "parser":  "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint", "jest"],
  "extends":  [
    "plugin:@typescript-eslint/recommended",
    "plugin:prettier/recommended",
    "prettier/@typescript-eslint"
  ],
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest/globals": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 9
  }
}
