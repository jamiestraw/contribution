{
  "name": "contribution",
  "description": "GitHub contribution streak & stat fetcher with zero dependencies",
  "version": "4.0.1",
  "author": "Jamie Weavis <jamie.weavis@gmail.com>",
  "types": "lib/index.d.ts",
  "main": "lib/index.js",
  "homepage": "https://github.com/jamieweavis/contribution",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jamieweavis/contribution/issues"
  },
  "files": [
    "lib/**/*"
  ],
  "keywords": [
    "contribution",
    "contribution-graph",
    "fetcher",
    "github",
    "no-dependencies",
    "stat",
    "statistics",
    "streak"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jamieweavis/contribution.git"
  },
  "devDependencies": {
    "@types/jest": "25.1.4",
    "@types/node": "13.9.3",
    "@typescript-eslint/eslint-plugin": "2.24.0",
    "@typescript-eslint/parser": "2.24.0",
    "codecov": "3.7.1",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-jest": "23.8.2",
    "eslint-plugin-prettier": "3.1.2",
    "husky": "4.2.3",
    "jest": "25.1.0",
    "lint-staged": "10.0.8",
    "prettier": "2.0.1",
    "ts-jest": "25.2.1",
    "typescript": "3.8.3"
  },
  "scripts": {
    "coverage": "codecov",
    "format": "prettier --write 'src/**/*.ts'",
    "lint": "eslint 'src/**/*.ts'",
    "test": "jest",
    "build": "tsc",
    "dev": "tsc -w"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
