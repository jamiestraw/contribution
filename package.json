{
  "name": "contribution",
  "description": "GitHub contribution streak & stat fetcher with zero dependencies",
  "version": "4.0.2",
  "author": "Jamie Weavis <jamie.weavis@gmail.com>",
  "types": "lib/index.d.ts",
  "main": "lib/index.js",
  "homepage": "https://github.com/jamieweavis/contribution",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jamieweavis/contribution/issues"
  },
  "files": [
    "lib/**/*"
  ],
  "keywords": [
    "contribution",
    "contribution-graph",
    "fetcher",
    "github",
    "no-dependencies",
    "stat",
    "statistics",
    "streak"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jamieweavis/contribution.git"
  },
  "devDependencies": {
    "@types/jest": "26.0.14",
    "@types/node": "14.11.2",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.34.0",
    "codecov": "3.7.2",
    "eslint": "7.9.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-jest": "24.0.2",
    "eslint-plugin-prettier": "3.1.4",
    "husky": "4.3.0",
    "jest": "26.4.2",
    "lint-staged": "10.4.0",
    "prettier": "2.1.2",
    "ts-jest": "25.5.1",
    "typescript": "4.0.3"
  },
  "scripts": {
    "coverage": "codecov",
    "format": "prettier --write 'src/**/*.ts'",
    "lint": "eslint 'src/**/*.ts'",
    "test": "jest",
    "build": "tsc",
    "dev": "tsc -w"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
