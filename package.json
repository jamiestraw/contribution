{
  "name": "contribution",
  "description": "GitHub contribution streak & stat fetcher with zero dependencies",
  "version": "3.0.1",
  "author": "Jamie Weavis <jamie.weavis@gmail.com>",
  "bugs": {
    "url": "https://github.com/jamieweavis/contribution/issues"
  },
  "devDependencies": {
    "codecov": "3.5.0",
    "eslint": "6.0.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-prettier": "5.0.0",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-jest": "22.7.1",
    "jest": "24.8.0",
    "prettier": "1.18.2",
    "typescript": "^3.5.2"
  },
  "files": [
    "lib"
  ],
  "homepage": "https://github.com/jamieweavis/contribution#readme",
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  },
  "keywords": [
    "contribution",
    "contribution-graph",
    "fetcher",
    "github",
    "no-dependencies",
    "stat",
    "statistics",
    "streak"
  ],
  "license": "MIT",
  "main": "./lib/index.min.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/jamieweavis/contribution.git"
  },
  "scripts": {
    "coverage": "codecov",
    "format": "prettier --write '{src,tests}/**/*.js'",
    "lint": "eslint '{src,tests}/**/*.js'",
    "test": "jest",
    "build": "tsc",
    "dev": "tsc -w"
  },
  "dependencies": {
    "@types/node": "^12.0.10"
  }
}
