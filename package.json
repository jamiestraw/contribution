{
  "name": "contribution",
  "description": "GitHub contribution streak & stat fetcher with zero dependencies",
  "version": "3.0.1",
  "author": "Jamie Weavis <jamie.weavis@gmail.com>",
  "bugs": {
    "url": "https://github.com/jamieweavis/contribution/issues"
  },
  "devDependencies": {
    "@types/jest": "24.0.17",
    "@types/node": "12.7.1",
    "@typescript-eslint/eslint-plugin": "1.13.0",
    "@typescript-eslint/parser": "1.13.0",
    "codecov": "3.5.0",
    "eslint": "6.1.0",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-jest": "22.14.1",
    "eslint-plugin-prettier": "3.1.0",
    "husky": "3.0.2",
    "jest": "24.8.0",
    "lint-staged": "9.2.1",
    "prettier": "1.18.2",
    "ts-jest": "24.0.2",
    "typescript": "3.5.3"
  },
  "files": [
    "lib/**/*"
  ],
  "homepage": "https://github.com/jamieweavis/contribution#readme",
  "keywords": [
    "contribution",
    "contribution-graph",
    "fetcher",
    "github",
    "no-dependencies",
    "stat",
    "statistics",
    "streak"
  ],
  "license": "MIT",
  "main": "./lib/contribution.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/jamieweavis/contribution.git"
  },
  "scripts": {
    "coverage": "codecov",
    "format": "prettier --write 'src/**/*.ts'",
    "lint": "eslint 'src/**/*.ts'",
    "test": "jest",
    "build": "tsc",
    "dev": "tsc -w"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  }
}
